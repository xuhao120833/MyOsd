<?xml version="1.0" encoding="utf-8"?>

<com.color.notification.view.CustomFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/framelanya"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="false"
    tools:ignore="MissingDefaultResource">

    <com.color.notification.view.CustomLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/notification_center_item_lanya"
        android:layout_width="214dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="3dp"
        android:layout_marginRight="12dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/shapetop_notification_item"
        android:orientation="horizontal">

        <com.color.notification.view.CustomImageView
            android:id="@+id/Icon_lanya"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="6dp"
            android:layout_marginBottom="10dp">

        </com.color.notification.view.CustomImageView>

        <com.color.notification.view.CustomLinearLayout
            android:id="@+id/notification_center_item_content_lanya"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:orientation="vertical">

            <com.color.notification.view.CustomLinearLayout
                android:id="@+id/linear_lanya"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.color.notification.view.CustomTextView
                    android:id="@+id/appName_lanya"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="20px">

                </com.color.notification.view.CustomTextView>

                <com.color.notification.view.CustomTextView
                    android:id="@+id/time_lanya"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:textColor="@color/white"
                    android:textSize="20px">

                </com.color.notification.view.CustomTextView>


            </com.color.notification.view.CustomLinearLayout>

            <com.color.notification.view.CustomLinearLayout
                android:id="@+id/linear_lanya2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <com.color.notification.view.CustomSeekBar
                    android:id="@+id/seekbar_lanya"
                    android:layout_width="130dp"
                    android:layout_height="5dp"
                    android:paddingStart="0dp"
                    android:paddingEnd="0dp"
                    android:background="@android:color/transparent"
                    android:layout_gravity="center|left"
                    android:max="100"
                    android:min="0"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:progressDrawable="@drawable/custom_brightness_seekbar"
                    android:thumb="@null">

                </com.color.notification.view.CustomSeekBar>

                <com.color.notification.view.CustomTextView
                    android:id="@+id/seekbar_lanya_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center"
                    android:gravity="right|center"
                    android:textColor="@color/white"
                    android:text="0%"
                    android:textSize="20px">

                </com.color.notification.view.CustomTextView>

            </com.color.notification.view.CustomLinearLayout>

        </com.color.notification.view.CustomLinearLayout>

    </com.color.notification.view.CustomLinearLayout>
</com.color.notification.view.CustomFrameLayout>
