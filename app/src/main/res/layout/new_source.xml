<?xml version="1.0" encoding="utf-8"?>
<com.color.systemui.view.navibar.new_navibar_source.NewSourceFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_source_frame"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    tools:ignore="MissingDefaultResource">

    <com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout
        android:id="@+id/new_source_linear"
        android:background="@drawable/shapetop1"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_width="300dp"
        android:layout_height="250dp">

        <com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout
            android:id="@+id/First_line"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text"
                android:layout_width="260dp"
                android:layout_height="20dp"
                android:layout_marginStart="10dp"
                android:layout_gravity="center"
                android:gravity="start"
                android:text="@string/soure_change"
                android:textColor="#FFFFFF" />

            <ImageView
                android:id="@+id/X"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginEnd="10dp"
                android:layout_gravity="center"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="false"
                android:src="@drawable/x"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck">

            </ImageView>
        </com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout>

        <com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout
            android:id="@+id/Second_line"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/OPS"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_gravity="center"
                android:layout_marginStart="70dp"
                android:layout_marginEnd="30dp"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="false"
                android:src="@drawable/ops"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck">

            </ImageView>

            <ImageView
                android:id="@+id/Android"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_gravity="center"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="false"
                android:src="@drawable/android_select_useful"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck">

            </ImageView>
        </com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout>

        <com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout
            android:id="@+id/Third_line"
            android:layout_width="match_parent"
            android:layout_height="115dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/HDMI1"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_marginStart="70dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="false"
                android:src="@drawable/hdmi1"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck">

            </ImageView>

            <ImageView
                android:id="@+id/HDMI2"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_marginTop="10dp"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="false"
                android:src="@drawable/hdmi2"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck">

            </ImageView>
        </com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout>

    </com.color.systemui.view.navibar.new_navibar_source.NewSourceLinearLayout>

</com.color.systemui.view.navibar.new_navibar_source.NewSourceFrameLayout>